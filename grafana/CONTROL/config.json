{
	"general":{
		"package":"grafana",
		"name":"Grafana",
		"version":"8.3.3-r0",
		"depends":[
			"docker-ce(>=19.03.8.r5)"
		],
		"developer":"Grafana Labs",
		"maintainer":"Donavan",
		"email":"snark@hey.com",
		"website":"https://grafana.com/",
		"architecture":"x86-64",
		"firmware":"3.5.0"
	},
	"adm-desktop": {
		"app": {
			"port": 3000,
			"protocol": "http",
